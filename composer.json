{
    "name": "denisyfrolov/json-serializer",
    "description": "Simple PHP serializer for any objects to JSON.",
    "keywords": ["json", "serialize", "serializer"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Frolov",
            "email": "denisyfrolov@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.19",
        "phpunit/phpunit": "^6.5"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "JsonSerializer\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JsonSerializer\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text -c phpunit.xml"
    }
}
